<!-- <!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd"> -->

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="engine" class="beans.Engine">
		<property name="modelYear" value="2019"></property>
	</bean>
	<bean id="engine1" class="beans.Engine">
		<property name="modelYear" value="2020"></property>
	</bean>
	<bean id="engine2" class="beans.Engine">
		<property name="modelYear" value="2021"></property>
	</bean>
</beans>